  "name": "react-babel",
  "version": "0.0.1",
  "description": "A boilerplate for getting a web application off the ground using React and Babel for JSX.",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack && node server.js",
    "dev": "webpack && webpack-dev-server --open --mode development",
    "dev-local-server": "PORT=36000 concurrently \"npm run dev\" \"node server.js\""
  },
  "dependencies": {
    "@feathersjs/express": "^1.3.0",
    "@feathersjs/feathers": "^3.3.0",
    "@feathersjs/socketio": "^3.2.8",
    "express": "^4.15.3",
    "feathers-memory": "^3.0.0",
    "grommet": "^2.2.1",
    "grommet-icons": "^4.1.0",
    "monaco-editor": "^0.15.6",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-register-cli": "^4.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": "8.1.x"
  },
  "repository": {
